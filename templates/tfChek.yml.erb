---
#This is a new configuration file

#Turn on Debug mode
debug:  <%= databag_config('com.wixpress.system.tfchek','debug') %>
# tfResDif timeout value in seconds
timeout:  <%= databag_config('com.wixpress.system.tfchek','timeout') %>
dismiss_out:  <%= databag_config('com.wixpress.system.tfchek','dismiss_out') %>
run_dir:  <%= databag_config('com.wixpress.system.tfchek','run_dir') %>
avatar_dir: <%= databag_config('com.wixpress.system.tfchek','avatar_dir')%>
repo_dir: <%= databag_config('com.wixpress.system.tfchek','repo_dir')%>
repo_owner: <%= databag_config('com.wixpress.system.tfchek','repo_owner') %>
qlength:  <%= databag_config('com.wixpress.system.tfchek','qlength') %>
webhook_secret:  <%= databag_passwd('com.wixpress.system.tfchek','webhook_secret') %>
token:  <%= databag_passwd('com.wixpress.system.tfchek','token') %>
github_cid: <%= databag_config('com.wixpress.system.tfchek','github_client_id')%>
github_cs: <%= databag_passwd('com.wixpress.system.tfchek','github_client_secret')%>
oauth_app_name: <%= databag_config('com.wixpress.system.tfchek','oauth_app_name')%>
oauth_home_page: <%= databag_config('com.wixpress.system.tfchek','oauth_home_page')%>
condom: <%= databag_config('com.wixpress.system.tfchek','condom')%>
#S3 bucket settings to store output of run.sh executions
#S3 bucket name
aws_s3_bucket_name: <%= databag_config('com.wixpress.system.tfchek','s3_bucket_name')%>
#S3 bucket region
aws_region: <%= databag_config('com.wixpress.system.tfchek','s3_region')%>
#Credentials to access S3 bucket
aws_access_key: <%= databag_passwd('com.wixpress.system.tfchek','s3_access_key')%>
aws_secret_key: <%= databag_passwd('com.wixpress.system.tfchek','s3_secret_key')%>


